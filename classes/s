$(document).ready(function() { $("#btn-close-ads").click(function() { $('#wallads').fadeOut(400); }); });

$(document).ready(function() {
    $("#smlsearch").hide();

    $(function() {
        $(window).scroll(function() {
            if ($(this).scrollTop() > 300) {
                $('#smlsearch').fadeIn();
            } else {
                $('#smlsearch').fadeOut();
            }
        });

        $('#smlsearch b').click(function() {
            $('body,html').animate({
                scrollTop: 0
            }, 800);

            return false;
        });
    });
});
$(document).ready(function() {
    $(function() {
        $('#topweb b').click(function() {
            $('body,html').animate({
                scrollTop: 0
            }, 2100);

            return false;
        });
    });
});


