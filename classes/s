function nsource() {
    var xhttp = new XMLHttpRequest();

    xhttp.onreadystatechange = function() {
        if (this.readyState == 4 && this.status == 200){
            document.getElementById("nsource").innerHTML = this.responseText;
        }
    };

    xhttp.open("GET", "http://www.programmingcenter.ir/des/php/source.php", true);
    xhttp.send();
}

